<%= form_for inquiry do |f| %>
  <% if inquiry.errors.any? %>
    <ul>
      <% inquiry.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  <% end %>
  
  <div>
    <%= f.label :name, :class => "contact_objects" %><br/>
    <%= f.text_field :name, :class => "contact_fields" %>
  </div>
  
  <div>
    <%= f.label :email, :class => "contact_objects" %><br/>
    <%= f.text_field :email, :class => "contact_fields" %>
  </div>
  
  <div>
    <%= f.label :message, :class => "contact_objects" %><br/>
    <%= f.text_area :message, :class => "contact_message" %>
  </div>
  
  <div >
    <%= f.submit "Send", :class => "contact_objects"%>
  </div>
<% end %>
